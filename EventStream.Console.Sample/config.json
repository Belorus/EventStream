// Data types: string, long, int, float, double, bool
// # - values are passed by called
// $ - evaluated via delegate

{
  "fields": {
    "user_id": "#string",
    "platform_type": "#string",
    "session_id": "#string",
    "user_sn_id": "#string",
    "login_mode": "#string",
    "app_version": "#string",
    "os_name": "#string",
    "os_version": "#string",

    "timestamp": "$long"
  },
  "groups": [
    {
      "percent": 2,
      "fields": {
        "event_group": "BBNC_CLIENT_INSTRUMENTATION"
      },
      "groups": [
        {
          "fields": {
            "event_sub_group": "AUTH"
          },
          "events": [
            {
              "id": "LOGGED_IN",
              "fields": { "event_type": "LOGGED_IN" }
            }
          ]
        }
      ]
    }
  ]
}
